<% provide(:title, "Register a new admin user") %>
<div id="clearance" class="page sign-up">
  <h2><%= yield(:title) %></h2>

  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="submit-field">
      <%= form.submit(class: 'button small') %>
    </div>
  <% end %>
</div>
